public class point{
     
     int ordone;
     int absise;

    public point( int ordone ,int absise) {
        this.ordone = ordone;
        this.absise = absise;
        
    }

    public float  norme ( ) {
         return Math.sqrt ((this.ordone x this.ordone) +  (this.absise x  this.absise));
    }
    public  float  distance (int A,int B ) {
         return Math.sqrt ((this.ordone -A)  *  * 2  +  (this.absise -B)  *  * 2);
    }
